{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off",
    "react/no-children-prop": "off",
    "no-extra-boolean-cast": "off"
  },
  "globals": {
    "process": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
