{
  "kind": "collectionType",
  "collectionName": "programs",
  "info": {
    "name": "Program",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "level": {
      "type": "string",
      "required": true
    },
    "form": {
      "type": "string",
      "required": false
    },
    "specialization": {
      "type": "text",
      "required": true
    },
    "term": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "cards": {
      "type": "dynamiczone",
      "components": [
        "card.card-block",
        "article.article"
      ],
      "required": true
    },
    "location": {
      "model": "location"
    },
    "program": {
      "type": "string",
      "required": true
    },
    "photos": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    }
